    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/multer": "^1.4.11",
        "@types/node": "^20.17.12",
        "@types/pg": "^8.10.9",
        "@types/uuid": "^9.0.8",
        "nodemon": "^3.0.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.7.2",
        "vitest": "^1.6.0"
