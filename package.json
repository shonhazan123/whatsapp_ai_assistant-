    "description": "WhatsApp AI Assistant with Calendar, Gmail, and Database integration",
    "main": "dist/index.js",
    "scripts": {
        "build": "cd Memo_v2 && npm run build",
        "start": "node Memo_v2/dist/server.js",
        "dev": "npm run build && nodemon Memo_v2/src/server.ts",
        "test": "cd Memo_v2 && npm test",
        "db:migrate": "node dist/scripts/migrate.js",
        "dev:ngrok": "node scripts/start-with-ngrok.js",
        "test:webhook": "node scripts/test-webhook.js",
        "whatsapp:info": "node scripts/get-whatsapp-info.js",
        "debug": "node scripts/debug-setup.js",
        "debug:morning-digest": "ts-node scripts/debug-morning-digest.ts"
    },
    "keywords": [
        "whatsapp",
        "ai",
        "assistant",
        "calendar",
        "gmail"
    ],
    "author": "",
    "license": "ISC",
